{
  "schemaVersion": 1,
  "id": "twilightforest",
  "version": "${version}",
  "name": "The Twilight Forest",
  "description": "An enchanted forest dimension.",
  "authors": [
    "Benimatic",
    "AtomicBlom",
    "Drullkus",
    "Killer_Demon",
    "quadraxis",
    "Tamaized",
    "williewillus",
    "GizmoTheMoonPig",
    "AlphaMode"
  ],
  "contact": {},
  "license": "GNU LESSER GENERAL PUBLIC LICENSE Version 2.1",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "twilightforest.client.TFClientSetup"
    ],
    "main": [
      "twilightforest.TwilightForestMod"
    ],
    "fabric-datagen": [
      "twilightforest.data.DataGenerators"
    ],
    "mm:early_risers": [
      "twilightforest.asm.TFASM"
    ],
    "cardinal-components": [
      "twilightforest.capabilities.CapabilityList"
    ],
    "rei": [
      "twilightforest.compat.rei.REICompat"
    ],
    "emi": [
      "twilightforest.compat.emi.EMICompat"
    ],
    "jei_mod_plugin": [
      "twilightforest.compat.jei.JEICompat"
    ]
  },
  "mixins": [
    "twilight.mixins.json"
  ],
  "accessWidener": "twilightforest.accesswidener",
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": ">=0.53.3",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": ">=2.0.2"
  },
  "custom": {
    "/* you need to enable this (and move it out of 'custom') if you want to run datagen because yes */ provides": [
      "com_electronwill_night-config_core",
      "com_electronwill_night-config_toml"
    ],
    "cardinal-components": [
      "twilightforest:cap_shield",
      "twilightforest:cap_feather_fan_fall",
      "twilightforest:cap_thrown"
    ],
    "banner": "twilightforest_logo.png"
  }
}