{
  "multipart": [{
    "apply": { "model": "twilightforest:stone_twist", "x": 90 }
  }, {
    "when": { "east": true },
    "apply": { "model": "twilightforest:stone_twist_extension", "x": 90 }
  }, {
    "when": { "south": true },
    "apply": { "model": "twilightforest:stone_twist_extension", "x": 90, "y": 90 }
  }, {
    "when": { "west": true },
    "apply": { "model": "twilightforest:stone_twist_extension", "x": 90, "y": 180 }
  }, {
    "when": { "north": true },
    "apply": { "model": "twilightforest:stone_twist_extension", "x": 90, "y": 270 }
  }, {
    "when": { "up": false },
    "apply": { "model": "twilightforest:stone_twist_top", "x": 90 }
  }, {
    "when": { "up": true, "east": true },
    "apply": { "model": "twilightforest:stone_twist_extension_top", "x": 90 }
  }, {
    "when": { "up": true, "south": true },
    "apply": { "model": "twilightforest:stone_twist_extension_top", "x": 90, "y": 90 }
  }, {
    "when": { "up": true, "west": true },
    "apply": { "model": "twilightforest:stone_twist_extension_top", "x": 90, "y": 180 }
  }, {
    "when": { "up": true, "north": true },
    "apply": { "model": "twilightforest:stone_twist_extension_top", "x": 90, "y": 270 }
  }, {
    "when": { "down": false },
    "apply": { "model": "twilightforest:stone_twist_bottom", "x": 90 }
  }, {
    "when": { "down": true, "east": true },
    "apply": { "model": "twilightforest:stone_twist_extension_bottom", "x": 90 }
  }, {
    "when": { "down": true, "south": true },
    "apply": { "model": "twilightforest:stone_twist_extension_bottom", "x": 90, "y": 90 }
  }, {
    "when": { "down": true, "west": true },
    "apply": { "model": "twilightforest:stone_twist_extension_bottom", "x": 90, "y": 180 }
  }, {
    "when": { "down": true, "north": true },
    "apply": { "model": "twilightforest:stone_twist_extension_bottom", "x": 90, "y": 270 }
  }]
}