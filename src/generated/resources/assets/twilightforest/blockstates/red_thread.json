{
  "multipart": [
    {
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true,
        "x": 180
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true,
        "x": 90,
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true,
        "x": -90,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true,
        "x": -90
      },
      "when": {
        "north": "true"
      }
    }
  ]
}