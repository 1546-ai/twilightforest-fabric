{
  "multipart": [
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "twilightforest:block/red_thread",
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "twilightforest:block/red_thread",
        "x": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "twilightforest:block/red_thread",
        "x": 90,
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "twilightforest:block/red_thread",
        "x": -90,
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "twilightforest:block/red_thread",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "twilightforest:block/red_thread",
        "x": -90,
        "uvlock": true
      }
    }
  ]
}